<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="com.example.zalert"
    android:versionCode="1"
    android:versionName="1.0" >

    <uses-sdk
        android:minSdkVersion="12"
        android:targetSdkVersion="18" />

    <uses-permission android:name="android.permission.INTERNET" />

    <application
        android:name="com.example.zalert.ParseApplication"
        android:icon="@drawable/ic_launcher"
        android:debuggable="true"
        android:label="@string/app_name" >
        <activity
            android:name="com.example.zalert.MainActivity"
            android:label="@string/app_name" >
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>
        <activity android:name="com.example.zalert.LoginSignUpActivity" >
        </activity>
        <activity android:name="com.example.zalert.signup" >
        </activity>
        <activity android:name="com.example.zalert.Welcome" >
        </activity>
        <activity android:name="com.example.zalert.CategoryActivity" >
        </activity>
        <activity
            android:name="com.example.zalert.SingleItem"
            android:label="@string/title_activity_single_item" >
        </activity>
    </application>

</manifest>